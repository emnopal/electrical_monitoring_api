{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": [
      2,
      "single"
    ],
    "no-unused-vars": "off",
    "linebreak-style": 0,
    "indent": [
      "error",
      4
    ],
    "new-cap": 0,
    "import/extensions": "off",
    "import/prefer-default-export": "off",
    "no-param-reassign": "off",
    "no-throw-literal": "off" // do not attempt
  }
}
